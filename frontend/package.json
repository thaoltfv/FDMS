{
  "name": "fdms-frontend",
  "version": "1.0.0",
  "description": "Fast Document Management System Frontend - Vue 3 + Ionic Mobile-First Interface",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@ionic/vue": "^7.6.0",
    "@ionic/vue-router": "^7.6.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7",
    "axios": "^1.6.2",
    "vue-i18n": "^9.8.0",
    "vee-validate": "^4.12.2",
    "yup": "^1.4.0",
    "chart.js": "^4.4.0",
    "vue-chartjs": "^5.3.0",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@ionic/cli": "^7.2.0",
    "@types/node": "^20.10.4",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.6",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "keywords": [
    "vue3",
    "ionic",
    "typescript",
    "document-management",
    "mobile-first",
    "pwa"
  ],
  "author": "FDMS Team",
  "license": "MIT"
}