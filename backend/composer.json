{
  "name": "yale-academy/yale-academy-backend",
  "type": "project",
  "keywords": [
    "framework",
    "laravel",
    "laravel-api",
    "laravel-skeleton",
    "laravel-ddd",
    "laravel-swoole",
    "swoole"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.4",
    "ext-bcmath": "*",
    "ext-intl": "*",
    "ext-json": "*",
    "ext-pcntl": "*",
    "ext-pdo": "*",
    "darkaonline/l5-swagger": "^8.0",
    "doctrine/dbal": "^2.0",
    "dompdf/dompdf": "^1.0",
    "fideloper/proxy": "^4.1",
    "fntneves/laravel-transactional-events": "^1.8",
    "fruitcake/laravel-cors": "v2.0.4",
    "guzzlehttp/guzzle": "^7.3",
    "ibrunotome/google2fa-laravel": "^1.0",
    "intervention/image": "^2.7",
    "jenssegers/agent": "^2.6",
    "kreait/firebase-tokens": "1.16.2",
    "kreait/laravel-firebase": "^3.1",
    "laravel/framework": "^7.24",
    "laravel/horizon": "^4.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.2",
    "league/flysystem-aws-s3-v3": "^1.0",
    "phpoffice/phpspreadsheet": "^1.24",
    "phpoffice/phpword": "^0.18.2",
    "phpviet/number-to-words": "^1.0",
    "predis/predis": "^2.0",
    "pusher/pusher-php-server": "~4.0",
    "rap2hpoutre/fast-excel": "^3.0",
    "spatie/laravel-activitylog": "^3.17",
    "spatie/laravel-permission": "^3.0",
    "spatie/laravel-query-builder": "^3.0",
    "sudiptpa/ipstack": "^1.3",
    "tymon/jwt-auth": "^1.0.0"
  },
  "require-dev": {
    "facade/ignition": "^2.0",
    "fzaninotto/faker": "^1.9.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.1",
    "nunomaduro/phpinsights": "^1.0",
    "elasticquent/elasticquent": "dev-master",
    "elasticsearch/elasticsearch": "^6.7.2",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "classmap": [
      "database/seeds"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "test": "phpunit --no-coverage",
    "phpcpd": "php phpcpd.phar app/ --fuzzy --progress",
    "static:analysis": "php artisan code:analyse --level=5",
    "insights": "php artisan insights --no-interaction --min-quality=90 --min-complexity=85 --min-architecture=90 --min-style=90",
    "lint": "php phpcs.phar",
    "lint:fix": "php phpcbf.phar"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "ext-pcntl": "7.4",
      "ext-posix": "7.4"
    },
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "minimum-stability": "RC",
  "prefer-stable": true
}