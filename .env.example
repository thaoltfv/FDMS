# Environment variables for FDMS development
# Copy this file to .env and modify as needed.

# PostgreSQL Database Settings
DB_USER=fdms_user
DB_PASSWORD=fdms_password
DB_NAME=fdms_db
DB_PORT=5432

# Garage S3 Storage Settings
S3_ACCESS_KEY=fdms_access_key
S3_SECRET_KEY=fdms_secret_key
S3_PORT=3900

# Application Ports
BACKEND_PORT=3000
FRONTEND_PORT=5173

# JWT Settings (for the backend)
JWT_SECRET=a-very-strong-and-long-secret-for-jwt
JWT_EXPIRATION=1d
